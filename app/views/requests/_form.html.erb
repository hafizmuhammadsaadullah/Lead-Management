<%= render 'error', request: @request %>
<%= f.label :user_id,'Engineer Name', class: "text-info my-1" %>
<%= f.select :user_id,@user.map {|u| [u.user_name, u.id]},{include_blank: 'Select Manager'}, { class: "form-control" } %>
<%= f.label :description, class: "text-info my-1" %>
<%= f.text_area :description, class: "form-control" %>
<%= f.submit  "Submit", class: "btn btn-info btn-md"  %>